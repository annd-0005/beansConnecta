<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="userDAO" class="vn.sun.DAO.client.impl.UserDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="jobTypeDAO"
		class="vn.sun.DAO.client.impl.JobTypeDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="companyDAO"
		class="vn.sun.DAO.client.impl.CompanyDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="candidateDAO"
		class="vn.sun.DAO.client.impl.CandidateDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="jobDAO"
		class="vn.sun.DAO.client.impl.JobDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
</beans>